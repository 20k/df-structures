<data-definition>
    <enum-type type-name='pattern_type'>
        <enum-item name='MONOTONE'/>
        <enum-item name='STRIPES'/>
        <enum-item name='IRIS_EYE'/>
        <enum-item name='SPOTS'/>
        <enum-item name='PUPIL_EYE'/>
    </enum-type>

    <struct-type type-name='descriptor_color' instance-vector='$global.world.raws.colors'>
        <stl-string name="id"/>
        <stl-vector name="word"/>
        <stl-vector name="unk_2c"/>
        <stl-string name="name"/>
        <int16_t name="unk_58"/>
        <s-float name="red"/>
        <s-float name="green"/>
        <s-float name="blue"/>
    </struct-type>

    <struct-type type-name='descriptor_shape' instance-vector='$global.world.raws.shapes'>
        <stl-string name="id"/>
        <stl-vector name="word"/>
        <stl-vector name="unk_2c"/>
        <stl-string name="name"/>
        <stl-string name="name_plural"/>
        <stl-vector name="adj"/>
        <int8_t name="tile"/>
    </struct-type>

    <struct-type type-name='descriptor_pattern' instance-vector='$global.world.raws.patterns'>
        <stl-string name='id'/>
        <stl-vector name="unk_1c"/>
        <enum base-type='int16_t' type-name='inclusion_type' name='pattern'/>
        <stl-vector name="cp_color"/>
    </struct-type>
</data-definition>

